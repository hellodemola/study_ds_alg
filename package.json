{
  "name": "enyata-react-base",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:start": "vite",
    "tests": "jest",
    "test": "jest -o --collectCoverage",
    "generalTest": "jest -o --collectCoverage",
    "watch": "jest --watch",
    "watchAll": "jest --watchAll",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "type-check": "tsc",
    "host": "vite --host",
    "dev": "concurrently \"npm run server:dev\" \"npm run start\"",
    "server:dev": "nodemon server.js",
    "server:build": "babel --verbose --out-dir dist --only 'server.js,src/lib/' src",
    "client:build": "vite build",
    "build": "npm run server:build && npm run client:build",
    "start": "node server.js",
    "preview": "vite preview",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@babel/preset-react": "^7.18.6",
    "compression": "^1.7.4",
    "concurrently": "^7.6.0",
    "express": "^4.18.2",
    "jest-environment-jsdom": "^29.5.0",
    "nodemon": "^2.0.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@storybook/addon-essentials": "^7.0.5",
    "@storybook/addon-interactions": "^7.0.5",
    "@storybook/addon-links": "^7.0.5",
    "@storybook/blocks": "^7.0.5",
    "@storybook/react": "^7.0.5",
    "@storybook/react-vite": "^7.0.5",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-css-modules": "^2.1.0",
    "jest-svg-transformer": "^1.0.0",
    "postcss": "^8.4.22",
    "prettier": "^2.8.7",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.5",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4",
    "vite": "^3.2.3",
    "vite-jest": "^0.1.4"
  }
}
